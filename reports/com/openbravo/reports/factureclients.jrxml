	<queryString>
		<![CDATA[SELECT RECEIPTS.DATENEW, TICKETS.TICKETID,
       PAYMENTS.TOTAL, CUSTOMERS.ADDRESS,
       CUSTOMERS.ADDRESS2,CUSTOMERS.POSTAL,CUSTOMERS.CITY,CUSTOMERS.NAME,
TAXLINES.BASE , TAXLINES.AMOUNT,
TAXLINES.BASE + TAXLINES.AMOUNT AS TOTAL_TICKET,
CASE
    WHEN TAXES.RATE = 0.055 THEN TAXLINES.AMOUNT
    ELSE 0.0
END
AS TVA5_5,
CASE
    WHEN TAXES.RATE = 0.196 THEN TAXLINES.AMOUNT
    ELSE 0.0
END
AS TVA19_6

FROM TICKETS, CUSTOMERS, PAYMENTS, TAXES, RECEIPTS
LEFT JOIN TAXLINES ON TAXLINES.RECEIPT = RECEIPTS.ID

WHERE
TAXES.ID = TAXLINES.TAXID AND
RECEIPTS.ID = TICKETS.ID AND
RECEIPTS.ID = PAYMENTS.RECEIPT AND
TICKETS.CUSTOMER = CUSTOMERS.ID AND
PAYMENTS.PAYMENT = 'debt'
ORDER BY CUSTOMERS.NAME,RECEIPTS.DATENEW ASC]]>
