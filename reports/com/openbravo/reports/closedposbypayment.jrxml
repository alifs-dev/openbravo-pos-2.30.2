	<queryString>
		<![CDATA[SELECT
CASE
    WHEN PAYMENTS.PAYMENT = 'cash' THEN 'Espece'
    WHEN PAYMENTS.PAYMENT = 'paperin' THEN 'Ticket resto'
    WHEN PAYMENTS.PAYMENT = 'cheque' THEN 'Cheque'
    WHEN PAYMENTS.PAYMENT = 'chequerefund' THEN 'Rendu Cheque'
    WHEN PAYMENTS.PAYMENT = 'cashrefund' THEN 'Rendu'
    WHEN PAYMENTS.PAYMENT = 'magcard' THEN 'CB'
    WHEN PAYMENTS.PAYMENT = 'magcardrefund' THEN 'Rendu CB'
    WHEN PAYMENTS.PAYMENT = 'free' THEN 'Offert'
    WHEN PAYMENTS.PAYMENT = 'cashin' THEN 'Entrée espèce'
    WHEN PAYMENTS.PAYMENT = 'cashout' THEN 'Sortie espèce'
    WHEN PAYMENTS.PAYMENT = 'debt' THEN 'Dette client'
    WHEN PAYMENTS.PAYMENT = 'debtpaid' THEN 'Paiement client'
    ELSE PAYMENTS.PAYMENT
END
AS PAYMENT,
SUM(PAYMENTS.TOTAL) AS TOTAL
FROM CLOSEDCASH, PAYMENTS, RECEIPTS
WHERE CLOSEDCASH.MONEY = RECEIPTS.MONEY AND PAYMENTS.RECEIPT = RECEIPTS.ID
GROUP BY  PAYMENTS.PAYMENT]]>
	</queryString>
